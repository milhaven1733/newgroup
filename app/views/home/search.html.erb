<h2>Search Result</h2>
<table class="table table-hover">
  <tbody>
    <% @tickets.each do |ticket| %>
      <tr>
        <td><%= ticket.name %></td>
        <td><%= ticket.category.try(:name) %></td>
        <td><%= ticket.amount %></td>
        <td><%= ticket.start_at %></td>
        <td><%= ticket.end_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
