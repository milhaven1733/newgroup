default: &default
  adapter: mysql2
  encoding: utf8
  username: root
  password:
  pool: 5
  timeout: 5000

development:
  <<: *default
  database: group_dev

# Warning: The database defined as "test" will be erased and
# re-generated from your development database when you run "rake".
# Do not set this db to the same as development or production.
test:
  <<: *default
  database: group_test

production:
  <<: *default
  database: group_prod
